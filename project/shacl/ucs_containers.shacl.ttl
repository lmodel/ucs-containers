            sh:order 7 ;
            sh:path <core:id> ],
        [ sh:class <core:Attribute> ;
            sh:description "connects any entity to an attribute" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path <core:hasAttribute> ] ;
    sh:targetClass <core:Association> .

